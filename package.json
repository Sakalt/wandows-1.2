{
  "homepage": "https://chrishtu.github.io/windows11",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public",
    "start": "tsdx watch --onSuccess \"cp dist/windows11.cjs.production.min.js public\"",
    "build": "tsdx build && cp dist/windows11.cjs.production.min.js public",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "windows11",
  "author": "chrishtu",
  "module": "dist/windows11.esm.js",
  "size-limit": [
    {
      "path": "dist/windows11.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/windows11.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "gh-pages": "^3.2.3",
    "husky": "^6.0.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
